{
    "name": "silverstripe/moduleratings-plugin",
    "description": "A Composer plugin to provide ability to rate your local modules",
    "type": "composer-plugin",
    "require": {
        "silverstripe/moduleratings": "^1@dev",
        "composer-plugin-api": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "composer/composer": "^2"
    },
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Robbie Averill",
            "email": "robbie@silverstripe.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "SilverStripe\\ModuleRatingsPlugin\\": "src/"
        }
    },
    "extra": {
        "class": "SilverStripe\\ModuleRatingsPlugin\\RatingsPlugin",
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
